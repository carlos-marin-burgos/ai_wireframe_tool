{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/54ad0b60-7fda-456b-b965-230c533f1418/v2.0",
          "clientIdSettingName": "AZURE_CLIENT_ID",
          "clientSecretSettingName": "AZURE_CLIENT_SECRET"
        },
        "userDetailsClaim": "preferred_username",
        "login": {
          "loginParameters": ["domain_hint=microsoft.com"]
        }
      }
    }
  },
  "routes": [
    {
      "route": "/.auth/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*.css",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*.js",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*.png",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*.jpg",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*.ico",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*.svg",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*.json",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/assets/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/submit-feedback",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/",
      "allowedRoles": ["authenticated"],
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      }
    },
    {
      "route": "/index.html",
      "allowedRoles": ["authenticated"],
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      }
    },
    {
      "route": "/*.html",
      "allowedRoles": ["authenticated"],
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      }
    },
    {
      "route": "/production-analytics.html",
      "allowedRoles": ["authenticated"],
      "headers": {
        "content-type": "text/html"
      }
    },
    {
      "route": "/simple-analytics.html",
      "allowedRoles": ["authenticated"],
      "headers": {
        "content-type": "text/html"
      }
    },
    {
      "route": "/monitor-analytics.html",
      "allowedRoles": ["authenticated"],
      "headers": {
        "content-type": "text/html"
      }
    },
    {
      "route": "/feedback-management.html",
      "allowedRoles": ["authenticated"],
      "headers": {
        "content-type": "text/html"
      }
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/production-analytics.html",
      "/simple-analytics.html",
      "/monitor-analytics.html",
      "/feedback-management.html",
      "/api/*",
      "/*.png",
      "/*.jpg",
      "/*.ico",
      "/*.svg",
      "/*.css",
      "/*.js",
      "/*.json"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad?post_login_redirect_uri=.referrer",
      "statusCode": 302
    }
  }
}
