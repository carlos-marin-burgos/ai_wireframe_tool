{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION=eastus}"
    },
    "openAiLocation": {
      "value": "${AZURE_OPENAI_LOCATION=eastus}"
    },
    "openAiSkuName": {
      "value": "${AZURE_OPENAI_SKU_NAME=S0}"
    },
    "gitHubUserName": {
      "value": "${GITHUB_USERNAME=carlosUX}"
    },
    "repositoryName": {
      "value": "${REPOSITORY_NAME=designetica}"
    },
    "repositoryUrl": {
      "value": "${REPOSITORY_URL=}"
    },
    "branch": {
      "value": "${BRANCH=main}"
    }
  }
}
